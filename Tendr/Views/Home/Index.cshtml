@model object

<!DOCTYPE html>

<html>
	<head>
		<title>Tendr</title>
	</head>
	<body>
		<div id="root"></div>
		@if (Context.Request.Host.Host == "app.tendr.local")
		{
			<script src="./assets/app.js"></script>
		}
		else
		{
			<script src="./assets/public.js"></script>
			<hr />
			<dl>
				@foreach (var claim in User.Claims)
				{
					<dt>@claim.Type</dt>
					<dd>@claim.Value</dd>
				}
			</dl>
		}
	</body>
</html>